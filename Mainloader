local gameid = game.GameId;
-- Blox Fruits
if (gameid == 2753915549) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/berhddb/PoltergeistHub/refs/heads/main/bloxfruits?token=GHSAT0AAAAAAC63RFAQ44QG4Z54ITZ3ZFIAZ5OKXRA", true))()
end
-- DW (Dandy's World)
if (gameid == 5569032992) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/jaonoobao/Poltergeist-Hub-DW-loader/refs/heads/main/README.md", true))()
end

-- Kick se n√£o for compativel
if (gameid ~= 5569032992) or (gameid ~= 2753915549) then
game.Players.LocalPlayer:Kick('Game not supported')
end
